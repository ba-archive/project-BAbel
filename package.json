{
  "name": "ba-publication-guidelines",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "vitepress dev docs",
    "open:dist": "open ./docs/.vitepress/dist",
    "build": "vitepress build docs && pnpm open:dist",
    "preview": "vitepress build docs && vitepress preview docs",
    "index:algolia": "docker run -it --env-file=config/.env -e 'CONFIG=$(cat config/algolia-config.json | jq -r tostring)' algolia/docsearch-scraper",
    "format": "prettier --config .prettierrc.json -uw ./docs/",
    "check": "vue-tsc --noEmit; eslint --config ./.eslintrc.json ./docs/ --ext .js,.ts,.vue"
  },
  "keywords": [],
  "author": "碧蓝档案资讯站",
  "license": "ISC",
  "devDependencies": {
    "@algolia/client-search": "4.14.3",
    "@rushstack/eslint-patch": "1.2.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vuepress/plugin-active-header-links": "2.0.0-beta.60",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-vue": "9.9.0",
    "naive-ui": "2.34.3",
    "prettier": "2.8.4",
    "typescript": "4.9.5",
    "vitepress": "1.0.0-alpha.46",
    "vue": "3.2.47",
    "vue-tsc": "1.1.7"
  }
}
